(describe "sanity tests"
  { it "checks variable equivalence" (should #t #t) }
  { it "checks assignment" (= {x} #t) (should x #t) }
  { it "checks re-assignment" (= {x} #f) (= {x} #t) (should x #t) }
  { it "checks function calling" (should (+ 1 1) 2) }
  { it "has higher order functions" 
    (= {adder} (\ {x} {+ x 100}))
    (= {result} (map adder {1 2 3 4 5}))
    (should result {101 102 103 104 105})
  }
  { it "has partial function application"
    (= {add} (\ {x y} {+ x y}))
    (= {add-10} (add 10)) 
    (should (add-10 20) 30)
  }
)